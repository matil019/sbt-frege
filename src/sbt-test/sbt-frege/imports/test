# check if the file gets created
$ absent target/frege/main/Main.java
$ absent target/frege/main/example/Hello.java
$ absent target/frege/main/example/Other.java
$ absent target/scala-2.10/classes/Main.class
$ absent target/scala-2.10/classes/example/Hello.class
$ absent target/scala-2.10/classes/example/Other.class
> compile
$ exists target/frege/main/Main.java
$ exists target/frege/main/example/Hello.java
$ exists target/frege/main/example/Other.java
$ exists target/scala-2.10/classes/Main.class
$ exists target/scala-2.10/classes/example/Hello.class
$ exists target/scala-2.10/classes/example/Other.class
